package banco;

public class Gerente extends Funcionario implements Autenticavel {
	private int senha;
	private int numeroDeFuncionariosGerenciados;
	
	Gerente() {
		//Construtor
	}
	
	
	
	public void setSenha(int senha) {
		this.senha = senha;
	}
	
	@Override
	public double getBonificacao() {
		return this.salario * 0.15;
	}
}